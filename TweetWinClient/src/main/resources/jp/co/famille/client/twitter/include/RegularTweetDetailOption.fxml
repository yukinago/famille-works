<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<AnchorPane id="AnchorPane" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <children>
    <Label layoutX="0.0" layoutY="0.0" text="詳細オプション" />
    <VBox layoutX="0.0" layoutY="15.0" padding="$x1" spacing="5.0" styleClass="border">
      <children>
        <Label text="ツイート順序" />
        <VBox spacing="5.0" styleClass="border">
          <children>
            <RadioButton mnemonicParsing="false" text="指定順序" />
            <RadioButton mnemonicParsing="false" text="ランダム順序" />
          </children>
          <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" fx:id="x1" />
          </padding>
        </VBox>
        <CheckBox mnemonicParsing="false" text="リピート" />
      </children>
    </VBox>
  </children>
</AnchorPane>
